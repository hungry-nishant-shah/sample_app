<% provide(:title, 'New Vendor') %>
<h1>Create New Speciality</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for [@vendor, @speciality] do |f| %>
      
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
