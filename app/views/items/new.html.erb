<% provide(:title, 'Create Menu') %>
<h1>Sign up</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for [@vendor,@menu,@item] do |f| %>
      
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :price %>
      <%= f.text_field :price %>

      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
